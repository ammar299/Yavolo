<% array_of_actions = connection_manager_actions_to_show(seller_api) %>
<% if array_of_actions.present? %>
  <% array_of_actions.each do |action| %>
    <%= link_to "#{action.humanize}", create_update_seller_api_confirmation_path(params[:controller], @seller, seller_api, action), remote: true, class: 'btn btn-sm btn-radius px-4 btn-secondary mb-1 current-action w-100', id: "#{seller_api.id}-#{action}" %>
  <% end %>
<% end %>
