<div class="main-page">

  <% featured_images_collage = render "admin/yavolos/manual_bundles/featured_image_pattern/featured_image_collage" %>

  <div class="main-content">
    <div class="container-fluid pdp-pad">
      <div class="px-xl-5 mt-5 ">
        <a href="#" class="red-text decoration-none btn-light-hvr btn-radius py-2 px-3 mr-5"><span
          class="icon-chevron-left-solid pr-2 mm-text"></span><strong>Back to results</strong></a>
        <span><a href="" class="decoration-none">Home</a>/<a href="" class="decoration-none">Page</a>/<a
          href="" class="decoration-none">Page</a>/<a href="" class="decoration-none">Page</a></span>
      </div>
      <div class="row mt-4 px-xl-5">
        <div class="col-xl-6">
          <div class="pdp-drag-slider">
            <div id="main-slider" class="splide splide-pad">
              <div class="splide__track">
                <div>
                  <span>
                      <%= image_tag "badge.png", class: "preview-exclusive-tag mr-0 mb-4 width-300" %>
                  </span>
                </div>
                <ul class="splide__list">
                  <li class="splide__slide">
                    <%= featured_images_collage %>
                  </li>
                  <% slider_images = @yavolo_bundle.pictures %>
                  <% slider_images.each do |image| %>
                    <li class="splide__slide px-lg-5">
                      <%= image_tag image.name.url %>
                    </li>
                  <% end %>
                  <%= image_tag 'default.jpg', class: 'w-100 d-none product-default-preview' %>
                </ul>
                <div class="text-right pr-3 pt-3">
                  <h6> A good deal better.</h6>
                </div>
              </div>
            </div>
            <div id="thumbnail-slider" class="splide">
              <div class="splide__track">
                <ul class="splide__list">
                  <li class="splide__slide slide-bundle-collage-image">
                    <%= featured_images_collage %>
                  </li>
                  <% slider_images = @yavolo_bundle.pictures %>
                  <% slider_images.each do |image| %>
                    <li class="splide__slide">
                      <%= image_tag image.name.url %>
                    </li>
                  <% end %>

                </ul>
              </div>
            </div>
          </div>


        </div>
        <div class="col-xl-6">
          <div class="single-product single-pdp-pad">
            <a href="" class="decoration-none red-text">
              <span class="icon-heart-Y">
                <svg xmlns="http://www.w3.org/2000/svg" width="22" height="20.808" viewBox="0 0 22 20.808">
                  <path id="Path_27" data-name="Path 27" d="M12.791,22.576l-1.45-1.308c-5.15-4.728-8.55-7.8-8.55-11.62a5.438,5.438,0,0,1,5.5-5.533,5.916,5.916,0,0,1,4.5,2.113,5.915,5.915,0,0,1,4.5-2.113,5.438,5.438,0,0,1,5.5,5.533c0,3.823-3.4,6.892-8.55,11.62Z" transform="translate(-1.791 -3.115)" fill="none" stroke="#ff3e55" stroke-width="2"/>
                </svg>
              </span>
            </a>
            <h4 class="black-text my-0"><%= @yavolo_bundle["title"] %></h4>
            <span class="mm-text"><strong>Sold by <%= get_sellers_of_yavolo_bundle(@yavolo_bundle) %> </strong></span>
            <ul class="rating d-flex mt-1">
              <li class="mr-1 active"><span class="icon-star"></span></li>
              <li class="mr-1 active"><span class="icon-star"></span></li>
              <li class="mr-1 active"><span class="icon-star"></span></li>
