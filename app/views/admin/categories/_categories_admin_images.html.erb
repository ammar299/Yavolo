<div class="modal fade" id="upload-images-popup" tabindex="-1" role="dialog" aria-labelledby="upload-images-popupLabel" aria-hidden="true">
<div class="modal-dialog" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title" id="upload-images-popupLabel">Upload Image</h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">Ã—</span>
      </button>
    </div>
    <div class="modal-body px-lg-4 pt-0">
      <div class="csv-file-uploader csv-file-uploader-ammar bg-white p-4 mb-4" >
          <% Picture.all.each do |pic| %>
            <span class="imglist">
              <a href="javascript:void(0)">
                <% if pic&.name&.url.present?%>
                  <%=image_tag pic&.name&.url, class: 'selected-image category-image',id: 'single-image' ,data: { picid: pic.id }%>
                <% end %>
              </a>
            </span>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
