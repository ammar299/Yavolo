$(".category-products-container").html("<%= j render( :partial => "product_in_category", :collection => @category_products, :as => :product) || render("no_product_in_category") %>")
$("#category_paginator").html("<%= j (paginate @category_products, remote: true, params: {controller: "/admin/categories", action: 'category_products_with_pagination'})  %>");
<% if params[:q].present? && params[:q][:title_or_brand_or_sku_or_yan_or_ean_cont].present? %>
  $(".icon-cross-container").removeClass("d-none")
  $(".icon-search-container").addClass("d-none")
<% else %>
  $(".icon-cross-container").addClass("d-none")
  $(".icon-search-container").removeClass("d-none")
  $(".category-products-search-term").val('')
<% end %>
