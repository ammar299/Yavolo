$('#delete-confirmation-modal').modal('hide');
if ($('#delivery-template-icons').find('i.delivery-template-cross-icon').length !== 0) {
    $('.delivery-template-search-field').val('');
    $('#delivery-template-icons').html('<i class="fa fa-search accounts-delivery-search" aria-hidden="true"></i>');
} else {
    $('#delivery-template-icons').html('<%= link_to '<i class="icon-cross delivery-template-cross-icon" aria-hidden="true"/>'.html_safe, search_seller_delivery_options_admin_delivery_options_path(seller_id: params[:seller_id]), method: :get, remote: true %>');
}
$('#sellers-delivery-options-list').html("<%= j render(partial: 'shared/admin/sellers/delivery_options_list', locals: { delivery_options: @delivery_options }) %>");