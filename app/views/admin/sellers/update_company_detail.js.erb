$("#error-message").text('');
<% unless @seller.errors.count > 0 %>
  $("#company-detail-content").html("<%= j render 'shared/sellers/company_detail', company_detail: @seller.company_detail %>");
  $('#editTemplateForm').modal('hide');
  $('#flash-msg').html('<%= j render partial: "shared/notice_banner" %>');
<% else %>
  $('#error-message').html("<%= @seller.errors.full_messages.join('<br/>').html_safe %>");
<% end %>
